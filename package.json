{
  "name": "react-cli4",
  "version": "0.0.1",
  "description": "React 16.0 boilerplate with react-router-dom, redux & webpack 4.x",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --config build/webpack.dev.conf.js",
    "build": "webpack -p --mode production --progress --config ./build/webpack.prod.conf.js",
    "analysis": "cross-env NODE_ENV=analysis webpack -p --mode production --progress --config ./build/webpack.prod.conf.js ",
    "depoly": "npm run build && node ./deploy.js",
    "test": "jest --coverage",
    "fix": "eslint --fix \"src/**/*.js\"",
    "format": "prettier --single-quote --write \"src/**/*.js\" && yarn fix",
    "precommit": "yarn format && lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "lint-staged": {
    "*.js": [
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dante-dan/react-sample-javascript.git"
  },
  "author": "duanjl <634344393@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dante-dan/react-sample/issues"
  },
  "homepage": "https://github.com/Dante-dan/react-sample#readme",
  "jest": {
    "coverageDirectory": "./test/coverage/",
    "moduleNameMapper": {},
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "resetMocks": true,
    "setupFiles": [],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "autoprefixer": "^9.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-babel-module": "5.0.0-beta.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^0.4.1",
    "node-notifier": "^5.2.1",
    "node-ssh": "^5.1.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.14.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "antd": "^3.7.0",
    "babel-polyfill": "^6.26.0",
    "js-cookie": "^2.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0"
  }
}
